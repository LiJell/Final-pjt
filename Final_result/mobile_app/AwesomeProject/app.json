{
  "expo": {
    "name": "LimnPet",
    "slug": "AwesomeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.limnpet.LiJell",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.limnpet.LiJell",
      "versionCode": 2,
      "permissions": [
      "CAMERA",
      "READ_EXTERNAL_STORAGE"
    ]
    }
  }
}
